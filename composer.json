{
    "name": "konekt/menu",
    "description": "Menus in Laravel 5 - 8",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel"
    ],
    "support": {
        "issues": "https://github.com/artkonekt/menu"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        },
        {
            "name": "Lavary",
            "email": "mrl.8081@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "~5.5|~6.0|~7.0|~8.0",
        "illuminate/view": "~5.5|~6.0|~7.0|~8.0"
    },
    "autoload": {
        "psr-4": {
            "Konekt\\Menu\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.5|~4.0|~5.0|~6.0",
        "phpunit/phpunit" : "6.2 - 7.5|~8.0|~9.0",
        "mockery/mockery": "~1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\Menu\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Konekt\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Konekt\\Menu\\Facades\\Menu"
            }
        }
    }
}
